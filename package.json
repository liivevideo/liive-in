{
  "name": "liive.in.clients",
  "version": "0.0.5",
  "private": true,
  "engine": {
    "node:": "node 6.4.0",
    "npm": "~3.10.0"
  },
  "scripts": {
    "clean": "watchman watch-del-all",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "storybook": "start-storybook -p 9001 -c web/.storybook",
    "ios": "react-native run-ios",
    "build-ios": "react-native run-ios",
    "android": "react-native run-android",
    "build-android": "react-native run-android",
    "web": "webpack-dev-server --port 3000 --config web/webpack.config.dev.js --inline --hot --colors --quiet",
    "build-web": "webpack --config web/webpack.config.dev.js --inline --hot --colors --quiet",
    "chrome": "webpack-dev-server --port 3001  --config chrome/webpack.config.chrome.js --inline  --colors --quiet",
    "build-chrome": "webpack --config chrome/webpack.config.chrome.js --inline  --colors --quiet ",
    "test-shippable": "mkdir -p shippable/testresults && mocha --reporter xunit ./test/*.js > shippable/testresults/result.xml",
    "test-coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- -u tdd",
    "shippable-cobertura": "./node_modules/.bin/istanbul report cobertura --dir shippable/codecoverage/"
  },
  "dependencies": {
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-native": "^0.32.0",
    "react-native-web": "0.0.45",
    "react-native-web-webrtc": "0.0.7",
    "react-native-webrtc": "^0.11.0",
    "socket.io-client": "^1.3.7",
    "url-loader": "0.5.7"
  },
  "devDependencies": {
    "@kadira/storybook": "1.28.1",
    "babel-core": "6.9.1",
